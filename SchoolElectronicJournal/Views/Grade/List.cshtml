@model IEnumerable<Grade>
<h3 class="p-2 bg-primary text-white text-center">Students</h3>

@if (Model.Count() == 0)
{
    <div class="row">
        <div class="col text-center p-2">No Data</div>
    </div>
}
else
{

    @foreach (Grade p in Model)
    {
        <div class="row p-2">
            <div class="col">@p.Number</div>

            <div class="col"></div>
        </div>


    }
    

}
<div class="col">
    <a href="~/Student/Select?userid=@ViewBag.UserId&id=@ViewBag.LesId" type="submit" class="btn btn-primary">Back</a>
</div>
